{
  "name": "travel-amadeus-mcp",
  "version": "1.1.0",
  "description": "Model Context Protocol server for Amadeus Travel APIs - provides 50+ tools for flight search, hotel booking, activities, and travel intelligence",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "travel-amadeus-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "amadeus",
    "travel",
    "flights",
    "hotels",
    "model-context-protocol",
    "ai",
    "claude",
    "llm",
    "booking",
    "activities",
    "tourism",
    "travel-api",
    "flight-search",
    "hotel-booking"
  ],
  "author": "Prathushan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Prathush21/travel-amadeus-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/Prathush21/travel-amadeus-mcp/issues"
  },
  "homepage": "https://github.com/Prathush21/travel-amadeus-mcp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "amadeus": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
